import java.io.*;
import java.util.*;

public class SecretCowCode {
 static long N;
    static long recurse( long placeHolder, long origLength){
        if (placeHolder<origLength) return placeHolder;

        long length = origLength;
        while (length*2<=placeHolder){
            length*=2;
        }



        if (placeHolder ==length){
            return recurse(length-1, origLength);
        }else{
            return recurse(placeHolder-length-1, origLength);
        }

    }

    public static void main(String[] args) throws IOException {
        BufferedReader bf=new BufferedReader(new FileReader("cowcode.in"));
        PrintWriter out =new PrintWriter("cowcode.out");

        StringTokenizer st = new StringTokenizer(bf.readLine());

        String base = st.nextToken();
        N  = Long.parseLong(st.nextToken());




       // System.out.println(iterations+" "+total);
        long ret = recurse(N-1,  base.length());

        out.println(base.charAt((int) ret));
        out.close();
    }
}
